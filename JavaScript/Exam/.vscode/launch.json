{
    "version": "0.2.0",
    "configurations": [{

        "type": "node",
        "request": "launch",
        "name": "Launch Program",
        "program": "${workspaceRoot}/first.js",
        "cwd": "${workspaceRoot}"
    }],
    [{
        "type": "node",
        "request": "launch",
        "name": "Launch Extension",
        "runtimeExecutable": "${execPath}",
        "args": [
            "--extensionDevelopmentPath=${workspaceRoot}"
        ],
        "sourceMaps": true,
        "outFiles": [
            "${workspaceRoot}/out/**/*.js"
        ],
        "preLaunchTask": "npm"
    }]
}